{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
    "templateParts": [
        {
            "area": "header",
            "name": "site-header",
            "title": "Site Header"
        },
        {
            "area": "footer",
            "name": "site-footer",
            "title": "Site Footer"
        }
    ],
	"settings": {
        "color": {
            "palette": [
                {
                    "slug": "russian-violet",
                    "color": "#14003fff",
                    "name": "Russian Violet"
                },
                {
                    "slug": "white-smoke",
                    "color": "#f7f4f3ff",
                    "name": "White Smoke"
                },
                {
                    "slug": "penn-red",
                    "color": "#92140cff",
                    "name": "Penn Red"
                },
                {
                    "slug": "salmon-pink",
                    "color": "#f48498ff",
                    "name": "Salmon Pink"
                },
                {
                    "slug": "cool-gray",
                    "color": "#8789c0ff",
                    "name": "Cool Gray"
                },
                {
                    "slug": "navy-blue",
                    "color": "#240072",
                    "name": "Navy Blue"
                },
                {
                    "slug": "tropical-indigo",
                    "color": "#8F93EF",
                    "name": "Tropical Indigo"
                },
                {
                    "slug": "soot-black",
                    "color": "#0F0D0D",
                    "name": "Soot Black"
                }
            ],
            "gradients": [
                {
                    "name": "Cool Gray to Russian Violet",
                    "slug": "cool-gray-to-russian-violet",
                    "gradient": "linear-gradient(-180deg, #8789c0ff 0%, #14003fff 100%)"
                },
                {
                    "name": "Salmon Pink to Peen Red",
                    "slug": "salmon-pink-to-penn-red",
                    "gradient": "linear-gradient(180deg, #f48498ff 0%, #92140cff 70%)"
                },
                {
                    "name": "Navy Blue to Russian Violet",
                    "slug": "navy-blue-to-russian-violet",
                    "gradient": "linear-gradient(-180deg, #240072 0%, #14003fff 100%)"
                },
                {
                    "name": "Tropical Indigo to Cool Gray",
                    "slug": "tropical-indigo-to-cool-gray",
                    "gradient": "linear-gradient(180deg, #8F93EF 0%, #8789c0ff 100%)"
                }
            ]
        },
        "layout": {
            "contentSize": "840px"
        }
    },
    "customTemplates": [
        {
            "name": "empty-canvas",
            "postTypes": ["page"],
            "title": "Empty Canvas"
        }
    ]
}